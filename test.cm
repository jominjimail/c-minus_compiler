/* A Program to perform Euclid`s /*ddfdf*/
   Algorithm to computer gcd */

int gcd (int u, int v)
{
    if (v == 0) return u;
    else return gcd(v,u-u/v*v);
    if (v >= 1) return u;
    if (v <= 1) return u;
    if (v != 1) return u;
    if (v== 8*8) return u;
    if (v < 8) return u;




    if (v > 8)  return u;
    if (v / 8)  return u;
    if (v + 8)  return u;
    if (v - 8)  return u;
    /* u-u/v*v == u mod v */
    if (v * 8) return u;
}
void main(void)
{
    int x; int y;
    x = input(); y = input();
    output(gcd(x,y));
}
